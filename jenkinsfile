pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building'
            }
        }
        stage('test') {
            steps {
                echo 'testing'
            }
        }
        stage('execute ') {
            steps {
                sh './script2.sh'
                sh 'cat script2.sh'
            }
        }
    }
}
